{
  "name": "maasai-dictionary-frontend",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "tsc && vite build --outDir dist",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "jest",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@stomp/stompjs": "^7.0.1",
    "@vitejs/plugin-react": "^4.5.2",
    "axios": "^1.7.2",
    "jwt-decode": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.0.0",
    "jest": "^29.0.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "tailwindcss": "^4.1.10",
    "typescript": "~5.0.4",
    "vite": "^6.3.5"
  }
